{
  "name": "nuxt4-vuetify",
  "version": "1.0.0",
  "description": "Zero-config Vuetify 3 module for Nuxt 4 with performance optimizations.",
  "repository": "KHNexTech/nuxt-vuetify",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nuxt",
    "nuxt 4",
    "vuetify",
    "vuetify 3",
    "nuxt-module",
    "nuxt-vuetify"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "consola": "^3.4.2",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.13.0",
    "@nuxt/kit": "^4.3.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.0",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.3.0",
    "typescript": "~5.9.3",
    "vite-plugin-vuetify": "^2.1.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4",
    "vuetify": "^3.11.8"
  },
  "peerDependencies": {
    "vite-plugin-vuetify": "^2.1.3",
    "vuetify": "^3.11.7",
    "@date-io/date-fns": "^2.x",
    "@date-io/luxon": "^2.x",
    "@date-io/dayjs": "^2.x",
    "@date-io/moment": "^2.x",
    "@date-io/js-joda": "^2.x"
  },
  "peerDependenciesMeta": {
    "vuetify": {
      "optional": true
    },
    "vite-plugin-vuetify": {
      "optional": true
    },
    "@date-io/date-fns": {
      "optional": true
    },
    "@date-io/luxon": {
      "optional": true
    },
    "@date-io/dayjs": {
      "optional": true
    },
    "@date-io/moment": {
      "optional": true
    },
    "@date-io/js-joda": {
      "optional": true
    }
  },
  "overrides": {
    "@nuxt/test-utils": {
      "vitest": "$vitest"
    }
  },
  "pnpm": {
    "overrides": {
      "vitest": "$vitest",
      "glob": "^13.0.0",
      "parse-url": "^10.0.3"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
